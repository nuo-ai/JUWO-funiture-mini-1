@font-face {
  font-family: 'iconfont';
  /* 由于微信小程序限制，无法使用本地字体文件，我们使用base64编码的字体文件 */
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* 定义各个图标的编码 */
.icon-dollar:before {
  content: "\e698";
}

.icon-clean:before {
  content: "\e63e";
}

.icon-truck:before {
  content: "\e640";
}

.icon-recycle:before {
